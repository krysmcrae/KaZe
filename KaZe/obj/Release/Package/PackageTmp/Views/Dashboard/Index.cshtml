                                        <td>
                                            @Html.DisplayFor(modelItem => tick.AssignedToUser.FirstName)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => tick.SubmittedByUser.FirstName)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => tick.Created)
                                        </td>
