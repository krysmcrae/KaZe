                                    <th class="sorting" tabindex="0" rowspan="1" colspan="1" aria-label="Assigned To Developer: activate to sort column ascending">Assigned To</th>
                                    <th class="sorting" tabindex="0" rowspan="1" colspan="1" aria-label="Submitted By User: activate to sort column ascending">Submitted By</th>
