                                                </td>
                                                break;
                                            case "Demo Admin":
                                                <td>
                                                    @Html.ActionLink("Details", "Details", "Tickets", new { id = tick.Id }, null)
                                                </td>
                                                break;


                                        }
                                    </tr>
                                }
                            </tbody>
                        </table>
                    </div>
                </div>


            </div>
        </div>
    </div>
    #endregion
