@if (Model.role == "Global Admin" || Model.role == "Demo Admin" || Model.role == "Admin")
{
    #region Admin Dashboard
    #region Project & Ticket Tables
    <div class="row">
        <div class="col-md">
            <div class="card ">
                <h5 class="card-header bg-primary " data-toggle="collapse" href="#AllProjectsCollapseAdmin" aria-expanded="false" aria-controls="AllProjectsCollapseAdmin" style="color:azure">Projects Section</h5>
                <div class="collapse multi-collapse" id="AllProjectsCollapseAdmin">
                    <br />
                    <h4>All Projects</h4>
                    <div class="card-body">
                        <table class="table table-striped table-bordered myTables" style="width: auto">
                            <thead>
                                <tr role="row">
                                    <th class="sorting_asc" tabindex="0" rowspan="1" colspan="1" aria-sort="ascending" aria-label="Project Name: activate to sort column descending">Project</th>
                                    <th class="sorting" tabindex="0" rowspan="1" colspan="1" aria-label="Description: activate to sort column ascending">Description</th>
                                    <th></th>
                                </tr>
                            </thead>
                            <tbody>

                                @foreach (var proj in Model.projects)
                                {
                                    <tr>
                                        <td>
                                            @Html.DisplayFor(modelItem => proj.Name)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => proj.Description)
                                        </td>
                                        @switch (Model.role)
                                        {
                                            case "Global Admin":
                                            case "Admin":
                                                <td>
                                                    @Html.ActionLink("Edit", "Edit", "Projects", new { id = proj.Id }, null)
                                                    <br />
                                                    @Html.ActionLink("Details", "Details", "Projects", new { id = proj.Id }, null)
                                                </td>
                                                break;
                                            case "Demo Admin":
                                                <td>
                                                    @Html.ActionLink("Details", "Details", "Projects", new { id = proj.Id }, null)
                                                </td>
                                                break;
                                        }
                                    </tr>
                                }
                            </tbody>
                        </table>
                    </div>
                </div>


            </div>
        </div>
        <div class="col-md">
